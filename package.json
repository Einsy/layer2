{
  "name": "level2",
  "version": "0.1.0",
  "description": "A streaming API for Wi-Fi traffic monitoring.",
  "keywords": [
    "wifi",
    "802.11",
    "frame",
    "packet",
    "stream",
    "monitor",
    "capture",
    "injection",
    "pcap",
    "wireshark",
    "tcpdump"
  ],
  "main": "./src/js",
  "scripts": {
    "build-debug": "node-gyp configure && node-gyp build --debug",
    "clean": "rm -rf build node_modules",
    "benchmark-capture": "cd benchmarks && make && node capture.js",
    "benchmark-decode": "cd benchmarks && node decode.js",
    "test": "$(npm bin)/mocha"
  },
  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "deep-diff": "^0.3.0",
    "mocha": "^2.0.1",
    "pcap": "^1.2.0",
    "pcap-stream": "^0.1.1",
    "tmp": "0.0.24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mtth/level2.git"
  },
  "author": {
    "name": "Matthieu Monsch",
    "email": "monsch@alum.mit.edu"
  },
  "license": "MIT"
}
